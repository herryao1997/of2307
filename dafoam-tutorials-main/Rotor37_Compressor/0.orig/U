FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 100);

boundaryField
{
    blade
    {
        type fixedValue;
        value uniform (0 0 0);
    }
    hub
    {
        type fixedValue;
        value uniform (0 0 0);
    }
    shroud
    {
        type fixedValue;
        value uniform (0 0 0);
    }
    per1
    {
        type            cyclic;
    }
    per2
    {
        type            cyclic;
    }
    inlet
    {
        type pressureInletVelocity;
        value $internalField;
    }
    outlet
    {
        type inletOutlet;
        value $internalField;
        inletValue uniform (0 0 0);
    }

}


// ************************************************************************* //
